        .containers {
            position: relative;
            z-index: 1;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-wrap: wrap;
            margin: 40px 0;
        }

        .containers .card {
            position: relative;
            width: 300px;
            height: 400px;
            background: rgba(251, 251, 251, 0.421);
            margin: 20px;
            box-shadow: 0 15px 35px rgba(0, 0, 0, 0.2),
                10px 15px 20px rgba(0, 0, 0, 0.2),
                15px 15px 35px rgba(0, 0, 0, 0.2),
                0 15px 35px rgba(0, 0, 0, 0.2),
                0 15px 35px rgba(0, 0, 0, 0.2),
                0 15px 35px rgba(0, 0, 0, 0.2),
                inset 2px 2px rgba(255, 255, 255, 0.2),
                inset -2px -2px rgba(0, 0, 0, 0.2);
            border-radius: 15px;
            display: flex;
            justify-content: center;
            align-items: center;
            backdrop-filter: blur(10px);
        }

        .containers .card .content {
            position: relative;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            opacity: 0.5;
            transition: 0.5s;
        }

        .containers .card:hover .content {
            opacity: 1;
            transform: translateY(-20px);
        }

        .containers .card .content .imgBx {
            position: relative;
            width: 150px;
            height: 150px;
            border-radius: 50%;
            overflow: hidden;
            border: 10px solid rgba(0, 0, 0, 0.25);
        }

        .containers .card .content .imgBx img {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .containers .card .content .contentBox h3 {
            color: #fff;
            text-transform: uppercase;
            letter-spacing: 2px;
            font-weight: 500;
            font-size: 18px;
            text-align: center;
            margin: 20px 0 10px;
            line-height: 1.1em;
        }

        .containers .card .content .contentBox h3 span {
            font-size: 12px;
            font-weight: 300;
            text-transform: initial;
        }

        .containers .card .sci {
            position: absolute;
            bottom: 50px;
            display: flex;
        }

        .containers .card .sci li {
            list-style: none;
            margin: 0 10px;
            transform: translateY(40px);
            transition: 0.5s;
            opacity: 0;
            transition-delay: calc(0.1s * var(--i));

        }

        .containers .card .sci li a {
            color: #fff;
            font-size: 24px;
        }

        .containers .card:hover .sci li {
            transform: translateY(0px);
            opacity: 1;
        }
    </style>
</head>

<body>
    <%- include('partials/header') %>
    <section>
        <div class="containers">
            <div class="card">
                <div class="content">
                    <div class="imgBx"><img src="/images/R.png"></div>
                    <div class="contentBox">
                        <h3>Navdeep Singh<br><span>Full Stack Web Developer </span></h3>
                    </div>
                </div>
                <ul class="sci">
                    <li style="--i:1">
                        <a href="#"><ion-icon name="logo-facebook"></ion-icon></a>
                    </li>
                    <li style="--i:2">
                        <a href="#"><ion-icon name="logo-instagram"></ion-icon></a>
                    </li>
                    <li style="--i:3">
                        <a href="#"><ion-icon name="logo-linkedin"></ion-icon></a>
                    </li>

                </ul>
            </div>
            <div class="card">
                <div class="content">
                    <div class="imgBx"><img src="/images/R.png"></div>
                    <div class="contentBox">
                        <h3>Sanya Sachdeva<br><span>Front End Web Developer </span></h3>
                    </div>
                </div>

                <ul class="sci">
                    <li style="--i:1">
                        <a href="#"><ion-icon name="logo-facebook"></ion-icon></a>
                    </li>
                    <li style="--i:2">
                        <a href="#"><ion-icon name="logo-instagram"></ion-icon></a>
                    </li>
                    <li style="--i:3">
                        <a href="#"><ion-icon name="logo-linkedin"></ion-icon></a>
                    </li>

                </ul>
            </div>
        </div>
    </section>

</body>

</html>